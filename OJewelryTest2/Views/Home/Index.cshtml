@{
    ViewBag.Title = "Home Page";
}

<h1>Clients</h1>
<div class="row">
    <div class="col-sm-4">Client</div>
    <div class="col-sm-3">Email</div>
    <div class="col-sm-1">Phone</div>
    <div class="col-sm-4">Company</div>
</div>
<!-- Loop thru clients-->

@foreach (var m in ViewData.Model)
{
<div class="row">
    <div class="col-sm-4">@m.ClientName</div>
    <div class="col-sm-3">@m.ClientEmail</div>
    <div class="col-sm-1">@m.ClientPhone</div>
    <div class="col-sm-4">@m.CompanyName</div>
</div>
}